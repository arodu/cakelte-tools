{
    "name": "arodu/cakelte-tools",
    "description": "CakeLteTools plugin for CakePHP",
    "type": "cakephp-plugin",
    "license": "MIT",
    "require": {
        "php": ">=8.1",
        "arodu/cakelte": "dev-2.next-cake5", 
        "cakephp/cakephp": "^5.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.1.0"
    },
    "autoload": {
        "psr-4": {
            "CakeLteTools\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "CakeLteTools\\Test\\": "tests/",
            "Cake\\Test\\": "vendor/cakephp/cakephp/tests/"
        }
    }
}
