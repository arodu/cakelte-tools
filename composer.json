{
    "name": "arodu/cakelte-tools",
    "description": "CakeLteTools plugin for CakePHP",
    "type": "cakephp-plugin",
    "license": "MIT",
    "require": {
        "php": ">=8.1",
        "arodu/cakelte": "^2.0",
        "cakephp/cakephp": "^5.0"
    },
    "require-dev": {
        "cakephp/cakephp-codesniffer": "^5.0",
        "phpunit/phpunit": "^10.1.0",
        "phpstan/phpstan": "^1.10",
        "cakedc/cakephp-phpstan": "^3.1"
    },
    "autoload": {
        "psr-4": {
            "CakeLteTools\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "CakeLteTools\\Test\\": "tests/",
            "Cake\\Test\\": "vendor/cakephp/cakephp/tests/"
        }
    }
}
